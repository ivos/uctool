	<uc-group primary-actor="a1" type="system" visibility="black" scope="Full refcodes">
		<use-case code="UC101" refcode="rfc101" goal="UC 101 goal" level="cloud" />
		<use-case code="UC102" refcode="rfc102" goal="UC 102 goal" level="kite">
