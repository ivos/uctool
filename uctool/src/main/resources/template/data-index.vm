#if($ctx.single)
#else
<!DOCTYPE HTML>
<html>
<head>
<title>Data overview</title>
#set($rel-path="../")
#parse("template/head.vm")
</head>
<body>
#set($section="data")
#set($isSection="true")
#parse("template/navbar.vm")
#end
<h2>Data overview</h2>
<table class="table table-striped table-bordered table-hover table-condensed">
<thead>
<tr>
<th><i class="fa fa-table"></i> Data</th>
<th>Code</th>
<th>Category</th>
</tr>
</thead>
<tbody>
#foreach($data in $ctx.dataOuts.values())
<tr>
<td>#dataRef($data)</td>
<td>$data.code</td>
<td>$data.category</td>
</tr>
#end
</tbody>
</table>
#if(! $ctx.single)
#parse("template/footer.vm")
</body>
</html>
#end
