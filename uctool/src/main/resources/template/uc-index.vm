#if($ctx.single)
#else
<!DOCTYPE HTML>
<html>
<head>
<title>Use Cases overview</title>
#set($rel-path="../")
#parse("template/head.vm")
</head>
<body>
#set($section="uc")
#set($isSection="true")
#parse("template/navbar.vm")
#end
<h2>Use Cases overview</h2>
<table class="table table-striped table-bordered table-hover table-condensed">
<thead>
<tr>
<th><i class="fa fa-user"></i> Primary actor</th>
<th><i class="fa fa-bolt"></i> Goal</th>
<th>Level</th>
</tr>
</thead>
<tbody>
#foreach($uc in $ctx.useCaseOuts.values())
<tr>
<td>$uc.primaryActor.name</td>
<td>#ucRef($uc)</td>
<td align="center">
#if($uc.levelTitle)<img src="../resources/${uc.level}.gif" alt="$uc.levelTitle" title="$uc.levelTitle"/>
#else&nbsp;
#end
</td>
</tr>
#end
</tbody>
</table>
#if(! $ctx.single)
#parse("template/footer.vm")
</body>
</html>
#end
