		<use-case code="UC004" goal="UC 004 goal" level="sea">
			<success>
				<step handle="h1"></step>
			</success>
			<extensions>
				<condition step-ref="h1">
					<step>
						Call
						<uc-ref code="UC005">UC005</uc-ref>
						.
					</step>
				</condition>
			</extensions>
		</use-case>
		<use-case code="UC005" goal="UC 005 goal" level="fish" />
		<use-case code="UC006" goal="UC 006 goal" level="clam" />
